{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "../scripts",
    "alwaysStrict": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    //"noUnusedParameters": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "removeComments": false
  }
}
